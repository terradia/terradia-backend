{
  "family": "terradia-api",
  "containerDefinitions": [
    {
      "name": "sample-app",
      "image": "kartoza/postgis",
      "cpu": 128,
      "memoryReservation": 128,
      "portMappings": [
        {
          "containerPort": 5432,
          "protocol": "tcp"
        }
      ],
      "command": [
        "npm", "start"
      ],
      "essential": true
    }
  ]
}
